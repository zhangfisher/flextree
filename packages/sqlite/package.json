{
    "name": "flextree-sqlite-adapter",
    "type": "module",
    "version": "1.0.2",
    "description": "sqlite driver for flextree",
    "author": "zhangfisher",
    "homepage": "https://zhangfisher.github.io/flextree/",
    "repository": {
        "type": "git",
        "url": "git@github.com:zhangfisher/flextree.git"
    },
    "license": "MIT",
    "publishConfig": {

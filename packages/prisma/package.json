    "version": "1.0.1",
    "description": "prisma adapter for flextree",
    "author": "wxzhang@126.com",
    "license": "ISC",
    "keywords": [
        "flextree"
    ],
    "homepage": "https://zhangfisher.github.io/flextree/",
